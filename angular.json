{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "hyfaa",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "hyfaa": {
      "projectType": "application",
      "root": "apps/hyfaa",
      "sourceRoot": "apps/hyfaa/src",
      "prefix": "hyfaa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/hyfaa",
            "index": "apps/hyfaa/src/index.html",
            "main": "apps/hyfaa/src/main.ts",
            "polyfills": "apps/hyfaa/src/polyfills.ts",
            "tsConfig": "apps/hyfaa/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/hyfaa/src/favicon.ico", "apps/hyfaa/src/assets"],
            "styles": [
              "apps/hyfaa/src/styles.scss",
              "node_modules/ol/ol.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css"
            ],
            "allowedCommonJsDependencies": ["toggle-fullscreen"],
            "scripts": ["node_modules/bootstrap/dist/js/bootstrap.bundle.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/hyfaa/src/environments/environment.ts",
                  "with": "apps/hyfaa/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hyfaa:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hyfaa:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hyfaa:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hyfaa/src/**/*.ts",
              "apps/hyfaa/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/hyfaa"],
          "options": {
            "jestConfig": "apps/hyfaa/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hyfaa-e2e": {
      "root": "apps/hyfaa-e2e",
      "sourceRoot": "apps/hyfaa-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hyfaa-e2e/cypress.json",
            "tsConfig": "apps/hyfaa-e2e/tsconfig.e2e.json",
            "devServerTarget": "hyfaa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hyfaa:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/hyfaa-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui-map": {
      "projectType": "library",
      "root": "libs/ui/map",
      "sourceRoot": "libs/ui/map/src",
      "prefix": "ui",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/map/src/**/*.ts",
              "libs/ui/map/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/map"],
          "options": {
            "jestConfig": "libs/ui/map/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-time": {
      "projectType": "library",
      "root": "libs/ui/time",
      "sourceRoot": "libs/ui/time/src",
      "prefix": "ui",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/time/src/**/*.ts",
              "libs/ui/time/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/time"],
          "options": {
            "jestConfig": "libs/ui/time/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "feature-map": {
      "projectType": "library",
      "root": "libs/feature/map",
      "sourceRoot": "libs/feature/map/src",
      "prefix": "hydro-flow",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature/map/src/**/*.ts",
              "libs/feature/map/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/feature/map"],
          "options": {
            "jestConfig": "libs/feature/map/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
